{
  "name": "moyu",
  "displayName": "moyu-mode",
  "publisher": "moyum",
  "repository": "https://github.com/MoYuM/moyu-template",
  "description": "包含了个人常用功能的vscode插件",
  "icon": "images/icon.png",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Keymaps",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moyu.new file",
        "title": "new file"
      },
      {
        "command": "moyu.new component",
        "title": "new component"
      },
      {
        "command": "moyu.new component in global",
        "title": "new component in global"
      },
      {
        "command": "moyu.new page",
        "title": "new page"
      }
    ],
    "keybindings": [
      {
        "command": "moyu.select neareast word",
        "key": "cmd+e"
      },
      {
        "command": "moyu.select next word",
        "key": "shift+cmd+."
      },
      {
        "command": "moyu.select pervious word",
        "key": "shift+cmd+,"
      },
      {
        "command": "moyu.move to next bracket",
        "key": "shift+cmd+9"
      },
      {
        "command": "moyu.search mode",
        "key": "cmd+m"
      },
      {
        "command": "moyu.move up",
        "key": "alt+k"
      },
      {
        "command": "moyu.move down",
        "key": "alt+j"
      },
      {
        "command": "moyu.snippet mode",
        "key": "cmd+k"
      },
      {
        "command": "moyu.escape",
        "key": "escape",
        "when": "moyu.snippetActive || moyu.searchActive"
      },
      {
        "command": "moyu.backspace",
        "key": "backspace",
        "when": "moyu.snippetActive"
      },
      {
        "command": "moyu.up",
        "key": "up",
        "when": "moyu.snippetActive"
      },
      {
        "command": "moyu.down",
        "key": "down",
        "when": "moyu.snippetActive"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@babel/generator": "^7.17.9",
    "@babel/parser": "^7.17.9",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@babel/traverse": "^7.17.9"
  }
}