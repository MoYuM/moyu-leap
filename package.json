{
  "name": "moyu",
  "displayName": "moyu-template",
  "publisher": "moyum",
  "repository": "https://github.com/MoYuM/moyu-template",
  "description": "包含了个人常用功能的vscode插件",
  "icon": "images/icon.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moyu.new file",
        "title": "new file"
      },
      {
        "command": "moyu.new component",
        "title": "new component"
      },
      {
        "command": "moyu.new component in global",
        "title": "new component in global"
      },
      {
        "command": "moyu.new page",
        "title": "new page"
      },
      {
        "command": "moyu.analyze text",
        "title": "analyze text"
      }
    ],
    "keybindings": [
      {
        "command": "moyu.select word",
        "key": "cmd+,"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
